@model
nrows 1
ncols 1
layer base
categories immature mature
min_age 1
max_age 50
age_plus_group true
initial_year 1994
current_year 2008
final_year 2108
cell_length 1
initialisation_phases phase1 phase2
time_steps step_one step_two

@initialisation_phase phase1
years 200
processes recruitment maturation halfm halfm ageing

@initialisation_phase phase2
years 1
processes recruitment maturation halfm halfm ageing

@time_step step_one
processes recruitment maturation halfm fishing halfm

@time_step step_two
processes ageing

# population processes
@process ageing
type ageing
categories immature mature

@process recruitment
type constant_recruitment
categories immature
proportions 1.0
r0 997386
age 1

@process halfm
type constant_mortality_rate
categories immature mature
m 0.065 0.065
selectivities one one

@process fishing
type event_mortality
categories immature mature
years 1998 1999 2000 2001 2002 2003 2004 2005 2006 2007
layers fishing_1998 fishing_1999 fishing_2000 fishing_2001 fishing_2002 fishing_2003 fishing_2004 fishing_2005 fishing_2006 fishing_2007
u_max 0.99
selectivities fishingsel fishingsel
penalty event_mortality_penalty

@process maturation
type category_transition_rate
from immature
to mature
proportions 1.0
selectivities maturation

# definition of layers
@layer base
type numeric
@include "base.map"

@layer constant
type numeric
@include "constant.map"

@layer ssru
type categorical
@include "ssru.map"

@layer fishing_1998
type numeric
@include "fishing_1998.map"

@layer fishing_1999
type numeric
@include "fishing_1999.map"

@layer fishing_2000
type numeric
@include "fishing_2000.map"

@layer fishing_2001
type numeric
@include "fishing_2001.map"

@layer fishing_2002
type numeric
@include "fishing_2002.map"

@layer fishing_2003
type numeric
@include "fishing_2003.map"

@layer fishing_2004
type numeric
@include "fishing_2004.map"

@layer fishing_2005
type numeric
@include "fishing_2005.map"

@layer fishing_2006
type numeric
@include "fishing_2006.map"

@layer fishing_2007
type numeric
@include "fishing_2007.map"

# definition of selecivities
@selectivity one
type constant
c 1

@selectivity maturation
type logistic_producing
l 5
h 30
a50 8
ato95 3

@selectivity fishingsel
type logistic
a50 8
ato95 3
